    Default = Color3.fromRGB(255, 0, 0),
    Title = "ESP Color",
    Transparency = 0,
    Callback = function(Value)
        ESPColor = Value
        print("ESP color changed to:", Value)
        updateESPColor()
    end
})

-- ÐŸÐµÑ€ÐµÐ¼ÐµÐ½Ð½Ñ‹Ðµ Ð´Ð»Ñ Anti-Lag
local AntiLagEnabled = false
local OriginalMaterials = {}
local OriginalTextures = {}
local antiLagConnection = nil

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð±ÐµÐ·Ð¾Ð¿Ð°ÑÐ½Ð¾Ð³Ð¾ Ð¿Ñ€Ð¸Ð¼ÐµÐ½ÐµÐ½Ð¸Ñ Ð¸Ð·Ð¼ÐµÐ½ÐµÐ½Ð¸Ð¹ Ðº Ð¾Ð±ÑŠÐµÐºÑ‚Ñƒ
local function applyObjectSimplification(obj)
    if not obj or not obj.Parent then return end
    
    -- Ð”Ð»Ñ Ñ‡Ð°ÑÑ‚ÐµÐ¹ Ð¸ Ð¼ÐµÑˆÐµÐ¹
    if obj:IsA("Part") or obj:IsA("MeshPart") or obj:IsA("UnionOperation") then
        -- Ð¡Ð¾Ñ…Ñ€Ð°Ð½ÑÐµÐ¼ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»ÑŒÐ½Ñ‹Ð¹ Ð¼Ð°Ñ‚ÐµÑ€Ð¸Ð°Ð»
        if not OriginalMaterials[obj] then
            OriginalMaterials[obj] = obj.Material
        end
        
        -- ÐŸÑ€Ð¸Ð¼ÐµÐ½ÑÐµÐ¼ ÑƒÐ¿Ñ€Ð¾Ñ‰ÐµÐ½Ð½Ñ‹Ð¹ Ð¼Ð°Ñ‚ÐµÑ€Ð¸Ð°Ð»
        pcall(function()
            obj.Material = Enum.Material.Plastic
        end)
        
        -- Ð¡Ð¾Ñ…Ñ€Ð°Ð½ÑÐµÐ¼ Ð¸ ÑƒÐ±Ð¸Ñ€Ð°ÐµÐ¼ Ñ‚ÐµÐºÑÑ‚ÑƒÑ€Ñƒ (ÐµÑÐ»Ð¸ ÑÐ²Ð¾Ð¹ÑÑ‚Ð²Ð¾ ÑÑƒÑ‰ÐµÑÑ‚Ð²ÑƒÐµÑ‚)
        if not OriginalTextures[obj] then
            pcall(function()
                OriginalTextures[obj] = obj.TextureID
            end)
        end
        
        pcall(function()
            obj.TextureID = ""
        end)
        
        -- Ð£Ð±Ð¸Ñ€Ð°ÐµÐ¼ SurfaceAppearance
        pcall(function()
            local surfaceAppearance = obj:FindFirstChildOfClass("SurfaceAppearance")
            if surfaceAppearance then
                surfaceAppearance:Destroy()
            end
        end)
        
    -- Ð”Ð»Ñ Ð´ÐµÐºÐ°Ð»ÐµÐ¹
    elseif obj:IsA("Decal") then
        if not OriginalMaterials[obj] then
            OriginalMaterials[obj] = true -- Ð¼Ð°Ñ€ÐºÐµÑ€ Ñ‡Ñ‚Ð¾ Ð¾Ð±ÑŠÐµÐºÑ‚ Ð±Ñ‹Ð»
        end
        pcall(function()
            obj:Destroy()
        end)
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð±ÐµÐ·Ð¾Ð¿Ð°ÑÐ½Ð¾Ð³Ð¾ Ð²Ð¾ÑÑÑ‚Ð°Ð½Ð¾Ð²Ð»ÐµÐ½Ð¸Ñ Ð¾Ð±ÑŠÐµÐºÑ‚Ð°
local function restoreObject(obj)
    if not obj or not obj.Parent then return end
    
    if obj:IsA("Part") or obj:IsA("MeshPart") or obj:IsA("UnionOperation") then
        -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð¼Ð°Ñ‚ÐµÑ€Ð¸Ð°Ð»
        if OriginalMaterials[obj] then
            pcall(function()
                obj.Material = OriginalMaterials[obj]
            end)
        end
        
        -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ñ‚ÐµÐºÑÑ‚ÑƒÑ€Ñƒ
        if OriginalTextures[obj] then
            pcall(function()
                obj.TextureID = OriginalTextures[obj]
            end)
        end
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¿Ñ€Ð¸Ð¼ÐµÐ½ÐµÐ½Ð¸Ñ Anti-Lag
local function applyAntiLag()
    if AntiLagEnabled then
        -- ÐŸÑ€Ð¸Ð¼ÐµÐ½ÑÐµÐ¼ Ðº ÑÑƒÑ‰ÐµÑÑ‚Ð²ÑƒÑŽÑ‰Ð¸Ð¼ Ð¾Ð±ÑŠÐµÐºÑ‚Ð°Ð¼
        for _, obj in pairs(workspace:GetDescendants()) do
            applyObjectSimplification(obj)
        end
    else
        -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»ÑŒÐ½Ñ‹Ðµ Ð¼Ð°Ñ‚ÐµÑ€Ð¸Ð°Ð»Ñ‹ Ð¸ Ñ‚ÐµÐºÑÑ‚ÑƒÑ€Ñ‹
        for obj, _ in pairs(OriginalMaterials) do
            restoreObject(obj)
        end
        
        -- ÐžÑ‡Ð¸Ñ‰Ð°ÐµÐ¼ Ñ‚Ð°Ð±Ð»Ð¸Ñ†Ñ‹
        OriginalMaterials = {}
        OriginalTextures = {}
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¾Ð±Ñ€Ð°Ð±Ð¾Ñ‚ÐºÐ¸ Ð½Ð¾Ð²Ñ‹Ñ… Ð¾Ð±ÑŠÐµÐºÑ‚Ð¾Ð²
local function handleNewObjects(obj)
    if AntiLagEnabled then
        applyObjectSimplification(obj)
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¾Ð±Ð½Ð¾Ð²Ð»ÐµÐ½Ð¸Ñ Anti-Lag
local function updateAntiLag()
    if antiLagConnection then
        antiLagConnection:Disconnect()
        antiLagConnection = nil
    end
    
    if AntiLagEnabled then
        -- ÐŸÑ€Ð¸Ð¼ÐµÐ½ÑÐµÐ¼ Ðº ÑÑƒÑ‰ÐµÑÑ‚Ð²ÑƒÑŽÑ‰Ð¸Ð¼ Ð¾Ð±ÑŠÐµÐºÑ‚Ð°Ð¼
        applyAntiLag()
        
        -- ÐÐ°ÑÑ‚Ñ€Ð°Ð¸Ð²Ð°ÐµÐ¼ Ð¾Ð±Ñ€Ð°Ð±Ð¾Ñ‚ÐºÑƒ Ð½Ð¾Ð²Ñ‹Ñ… Ð¾Ð±ÑŠÐµÐºÑ‚Ð¾Ð²
        antiLagConnection = workspace.DescendantAdded:Connect(handleNewObjects)
        
        Library:Notify({
            Title = "Anti-Lag Enabled",
            Description = "Materials and textures simplified",
            Time = 3
        })
    else
        -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»Ñ‹
        applyAntiLag()
        
        Library:Notify({
            Title = "Anti-Lag Disabled",
            Description = "Original materials restored",
            Time = 3
        })
    end
end

-- Ð¢Ð¾Ð³Ð³Ð» Ð´Ð»Ñ Anti-Lag
local AntiLagToggle = VisualTab:AddToggle("AntiLag", {
    Text = "ðŸ’¢ Anti-Lag",
    Default = false,
    Callback = function(Value)
        AntiLagEnabled = Value
        print("Anti-Lag enabled:", Value)
        updateAntiLag()
    end
})

-- ÐŸÐµÑ€ÐµÐ¼ÐµÐ½Ð½Ñ‹Ðµ Ð´Ð»Ñ X-Ray
local XRayEnabled = false
local OriginalTransparency = {}
local xRayConnection = nil

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¿Ñ€Ð¾Ð²ÐµÑ€ÐºÐ¸, ÑÐ²Ð»ÑÐµÑ‚ÑÑ Ð»Ð¸ Ð¾Ð±ÑŠÐµÐºÑ‚ Ñ‡Ð°ÑÑ‚ÑŒÑŽ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð°
local function isPartOfCharacter(obj)
    local model = obj:FindFirstAncestorOfClass("Model")
    if model then
        local humanoid = model:FindFirstChildOfClass("Humanoid")
        if humanoid then
            return true
        end
    end
    return false
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¿Ñ€Ð¸Ð¼ÐµÐ½ÐµÐ½Ð¸Ñ X-Ray Ðº Ð¾Ð±ÑŠÐµÐºÑ‚Ñƒ
local function applyXRay(obj)
    if not obj or not obj.Parent then return end
    
    -- ÐžÐ±Ñ€Ð°Ð±Ð°Ñ‚Ñ‹Ð²Ð°ÐµÐ¼ Ñ‚Ð¾Ð»ÑŒÐºÐ¾ Ñ‡Ð°ÑÑ‚Ð¸, ÐºÐ¾Ñ‚Ð¾Ñ€Ñ‹Ðµ Ð½Ðµ ÑÐ²Ð»ÑÑŽÑ‚ÑÑ Ñ‡Ð°ÑÑ‚ÑŒÑŽ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð°
    if (obj:IsA("Part") or obj:IsA("MeshPart") or obj:IsA("UnionOperation") or obj:IsA("TrussPart")) and not isPartOfCharacter(obj) then
        local currentTransparency = obj.Transparency
        
        -- Ð•ÑÐ»Ð¸ Ñ‚ÐµÐºÑƒÑ‰Ð°Ñ Ð¿Ñ€Ð¾Ð·Ñ€Ð°Ñ‡Ð½Ð¾ÑÑ‚ÑŒ Ð¼ÐµÐ½ÑŒÑˆÐµ 0.5, Ñ‚Ð¾ Ð¼Ñ‹ ÐµÐµ Ð¼ÐµÐ½ÑÐµÐ¼
        if currentTransparency < 0.5 then
            -- Ð¡Ð¾Ñ…Ñ€Ð°Ð½ÑÐµÐ¼ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»ÑŒÐ½ÑƒÑŽ Ð¿Ñ€Ð¾Ð·Ñ€Ð°Ñ‡Ð½Ð¾ÑÑ‚ÑŒ, ÐµÑÐ»Ð¸ ÐµÑ‰Ðµ Ð½Ðµ ÑÐ¾Ñ…Ñ€Ð°Ð½Ð¸Ð»Ð¸
            if not OriginalTransparency[obj] then
                OriginalTransparency[obj] = currentTransparency
            end
            
            -- Ð£ÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð½Ð¾Ð²ÑƒÑŽ Ð¿Ñ€Ð¾Ð·Ñ€Ð°Ñ‡Ð½Ð¾ÑÑ‚ÑŒ
            pcall(function()
                obj.Transparency = 0.7 -- ÐœÐ¾Ð¶Ð½Ð¾ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¸Ñ‚ÑŒ ÑƒÑ€Ð¾Ð²ÐµÐ½ÑŒ Ð¿Ñ€Ð¾Ð·Ñ€Ð°Ñ‡Ð½Ð¾ÑÑ‚Ð¸
            end)
        end
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð²Ð¾ÑÑÑ‚Ð°Ð½Ð¾Ð²Ð»ÐµÐ½Ð¸Ñ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»ÑŒÐ½Ð¾Ð¹ Ð¿Ñ€Ð¾Ð·Ñ€Ð°Ñ‡Ð½Ð¾ÑÑ‚Ð¸ Ð¾Ð±ÑŠÐµÐºÑ‚Ð°
local function restoreObjectTransparency(obj)
    if OriginalTransparency[obj] then
        pcall(function()
            obj.Transparency = OriginalTransparency[obj]
        end)
        OriginalTransparency[obj] = nil
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¿Ñ€Ð¸Ð¼ÐµÐ½ÐµÐ½Ð¸Ñ X-Ray ÐºÐ¾ Ð²ÑÐµÐ¼ Ð¾Ð±ÑŠÐµÐºÑ‚Ð°Ð¼
local function applyXRayToAll()
    if XRayEnabled then
        -- ÐŸÑ€Ð¸Ð¼ÐµÐ½ÑÐµÐ¼ Ðº ÑÑƒÑ‰ÐµÑÑ‚Ð²ÑƒÑŽÑ‰Ð¸Ð¼ Ð¾Ð±ÑŠÐµÐºÑ‚Ð°Ð¼
        for _, obj in pairs(workspace:GetDescendants()) do
            applyXRay(obj)
        end
    else
        -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»ÑŒÐ½ÑƒÑŽ Ð¿Ñ€Ð¾Ð·Ñ€Ð°Ñ‡Ð½Ð¾ÑÑ‚ÑŒ
        for obj, _ in pairs(OriginalTransparency) do
            restoreObjectTransparency(obj)
        end
        OriginalTransparency = {}
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¾Ð±Ñ€Ð°Ð±Ð¾Ñ‚ÐºÐ¸ Ð½Ð¾Ð²Ñ‹Ñ… Ð¾Ð±ÑŠÐµÐºÑ‚Ð¾Ð² X-Ray
local function handleNewXRayObjects(obj)
    if XRayEnabled then
        applyXRay(obj)
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¾Ð±Ð½Ð¾Ð²Ð»ÐµÐ½Ð¸Ñ X-Ray
local function updateXRay()
    if xRayConnection then
        xRayConnection:Disconnect()
        xRayConnection = nil
    end
    
    if XRayEnabled then
        -- ÐŸÑ€Ð¸Ð¼ÐµÐ½ÑÐµÐ¼ Ðº ÑÑƒÑ‰ÐµÑÑ‚Ð²ÑƒÑŽÑ‰Ð¸Ð¼ Ð¾Ð±ÑŠÐµÐºÑ‚Ð°Ð¼
        applyXRayToAll()
        
        -- ÐÐ°ÑÑ‚Ñ€Ð°Ð¸Ð²Ð°ÐµÐ¼ Ð¾Ð±Ñ€Ð°Ð±Ð¾Ñ‚ÐºÑƒ Ð½Ð¾Ð²Ñ‹Ñ… Ð¾Ð±ÑŠÐµÐºÑ‚Ð¾Ð²
        xRayConnection = workspace.DescendantAdded:Connect(handleNewXRayObjects)
        
        Library:Notify({
            Title = "X-Ray Enabled",
            Description = "You can now see through walls",
            Time = 3
        })
    else
        -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»Ñ‹
        applyXRayToAll()
        
        Library:Notify({
            Title = "X-Ray Disabled",
            Description = "Walls are now opaque again",
            Time = 3
        })
    end
end

-- Ð¢Ð¾Ð³Ð³Ð» Ð´Ð»Ñ X-Ray
local XRayToggle = VisualTab:AddToggle("XRay", {
    Text = "ðŸ’« Xray",
    Default = false,
    Callback = function(Value)
        XRayEnabled = Value
        print("X-Ray enabled:", Value)
        updateXRay()
    end
})

-- Ð¡Ð»Ð°Ð¹Ð´ÐµÑ€ Ð´Ð»Ñ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸ Ð¿Ñ€Ð¾Ð·Ñ€Ð°Ñ‡Ð½Ð¾ÑÑ‚Ð¸ X-Ray
local XRayTransparencySlider = VisualTab:AddSlider("XRayTransparency", {
    Text = "X-Ray Transparency",
    Default = 70,
    Min = 10,
    Max = 90,
    Rounding = 0,
    Suffix = "%",
    Callback = function(Value)
        -- ÐžÐ±Ð½Ð¾Ð²Ð»ÑÐµÐ¼ Ð¿Ñ€Ð¾Ð·Ñ€Ð°Ñ‡Ð½Ð¾ÑÑ‚ÑŒ Ð´Ð»Ñ Ð²ÑÐµÑ… Ð¾Ð±ÑŠÐµÐºÑ‚Ð¾Ð², ÐµÑÐ»Ð¸ X-Ray Ð²ÐºÐ»ÑŽÑ‡ÐµÐ½
        if XRayEnabled then
            for obj, _ in pairs(OriginalTransparency) do
                if obj and obj.Parent then
                    pcall(function()
                        obj.Transparency = Value / 100
                    end)
                end
            end
        end
    end
})

-- ÐŸÐµÑ€ÐµÐ¼ÐµÐ½Ð½Ñ‹Ðµ Ð´Ð»Ñ FullBright
local FullBrightEnabled = false
local OriginalLightingSettings = {}
local lighting = game:GetService("Lighting")

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¿Ñ€Ð¸Ð¼ÐµÐ½ÐµÐ½Ð¸Ñ FullBright
local function applyFullBright()
    if FullBrightEnabled then
        -- Ð¡Ð¾Ñ…Ñ€Ð°Ð½ÑÐµÐ¼ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»ÑŒÐ½Ñ‹Ðµ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸ Ð¾ÑÐ²ÐµÑ‰ÐµÐ½Ð¸Ñ
        if not next(OriginalLightingSettings) then
            OriginalLightingSettings = {
                Ambient = lighting.Ambient,
                Brightness = lighting.Brightness,
                GlobalShadows = lighting.GlobalShadows,
                OutdoorAmbient = lighting.OutdoorAmbient,
                ClockTime = lighting.ClockTime,
                FogStart = lighting.FogStart,
                FogEnd = lighting.FogEnd,
                FogColor = lighting.FogColor,
                ExposureCompensation = lighting.ExposureCompensation
            }
        end
        
        -- ÐŸÑ€Ð¸Ð¼ÐµÐ½ÑÐµÐ¼ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸ FullBright
        pcall(function()
            lighting.Ambient = Color3.new(1, 1, 1) -- Ð‘ÐµÐ»Ñ‹Ð¹ ambient ÑÐ²ÐµÑ‚
            lighting.Brightness = 2 -- Ð£Ð²ÐµÐ»Ð¸Ñ‡Ð¸Ð²Ð°ÐµÐ¼ ÑÑ€ÐºÐ¾ÑÑ‚ÑŒ
            lighting.GlobalShadows = false -- ÐžÑ‚ÐºÐ»ÑŽÑ‡Ð°ÐµÐ¼ Ñ‚ÐµÐ½Ð¸
            lighting.OutdoorAmbient = Color3.new(1, 1, 1) -- Ð‘ÐµÐ»Ñ‹Ð¹ outdoor ambient
            lighting.ClockTime = 14 -- Ð£ÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð´Ð½ÐµÐ²Ð½Ð¾Ðµ Ð²Ñ€ÐµÐ¼Ñ
            lighting.FogStart = 100000 -- Ð£Ð±Ð¸Ñ€Ð°ÐµÐ¼ Ñ‚ÑƒÐ¼Ð°Ð½
            lighting.FogEnd = 100000
            lighting.ExposureCompensation = 1 -- ÐšÐ¾Ð¼Ð¿ÐµÐ½ÑÐ°Ñ†Ð¸Ñ ÑÐºÑÐ¿Ð¾Ð·Ð¸Ñ†Ð¸Ð¸
        end)
    else
        -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»ÑŒÐ½Ñ‹Ðµ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸
        if next(OriginalLightingSettings) then
            pcall(function()
                lighting.Ambient = OriginalLightingSettings.Ambient or Color3.new(0.5, 0.5, 0.5)
                lighting.Brightness = OriginalLightingSettings.Brightness or 1
                lighting.GlobalShadows = OriginalLightingSettings.GlobalShadows or true
                lighting.OutdoorAmbient = OriginalLightingSettings.OutdoorAmbient or Color3.new(0.5, 0.5, 0.5)
                lighting.ClockTime = OriginalLightingSettings.ClockTime or 14
                lighting.FogStart = OriginalLightingSettings.FogStart or 0
                lighting.FogEnd = OriginalLightingSettings.FogEnd or 100000
                lighting.FogColor = OriginalLightingSettings.FogColor or Color3.new(1, 1, 1)
                lighting.ExposureCompensation = OriginalLightingSettings.ExposureCompensation or 0
            end)
            OriginalLightingSettings = {}
        end
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¾Ð±Ð½Ð¾Ð²Ð»ÐµÐ½Ð¸Ñ FullBright
local function updateFullBright()
    applyFullBright()
    
    if FullBrightEnabled then
        Library:Notify({
            Title = "FullBright Enabled",
            Description = "Shadows removed and brightness increased",
            Time = 3
        })
    else
        Library:Notify({
            Title = "FullBright Disabled",
            Description = "Lighting restored to normal",
            Time = 3
        })
    end
end

-- Ð¢Ð¾Ð³Ð³Ð» Ð´Ð»Ñ FullBright
local FullBrightToggle = VisualTab:AddToggle("FullBright", {
    Text = "ðŸ”† FullBright",
    Default = false,
    Callback = function(Value)
        FullBrightEnabled = Value
        print("FullBright enabled:", Value)
        updateFullBright()
    end
})

-- Ð¡Ð»Ð°Ð¹Ð´ÐµÑ€ Ð´Ð»Ñ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸ ÑÑ€ÐºÐ¾ÑÑ‚Ð¸
local BrightnessSlider = VisualTab:AddSlider("BrightnessLevel", {
    Text = "Brightness Level",
    Default = 1,
    Min = 1,
    Max = 10,
    Rounding = 1,
    Callback = function(Value)
        if FullBrightEnabled then
            pcall(function()
                lighting.Brightness = Value
            end)
        end
    end
})

-- ÐŸÐµÑ€ÐµÐ¼ÐµÐ½Ð½Ñ‹Ðµ Ð´Ð»Ñ Stop Rendering
local StopRenderingEnabled = false
local originalSettings = {}
local blackScreen = nil

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ ÑÐ¾Ñ…Ñ€Ð°Ð½ÐµÐ½Ð¸Ñ Ð²ÑÐµÑ… Ñ‚ÐµÐºÑƒÑ‰Ð¸Ñ… Ð½Ð°ÑÑ‚Ñ€Ð¾ÐµÐº
local function saveOriginalSettings()
    local lighting = game:GetService("Lighting")
    local settings = settings()
    
    originalSettings = {
        -- ÐÐ°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸ Ð¾ÑÐ²ÐµÑ‰ÐµÐ½Ð¸Ñ
        Ambient = lighting.Ambient,
        Brightness = lighting.Brightness,
        GlobalShadows = lighting.GlobalShadows,
        OutdoorAmbient = lighting.OutdoorAmbient,
        ClockTime = lighting.ClockTime,
        FogStart = lighting.FogStart,
        FogEnd = lighting.FogEnd,
        FogColor = lighting.FogColor,
        ExposureCompensation = lighting.ExposureCompensation,
        
        -- ÐÐ°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸ Ð³Ñ€Ð°Ñ„Ð¸ÐºÐ¸
        QualityLevel = settings.Rendering.QualityLevel,
        EagerBulkExecution = settings.Rendering.EagerBulkExecution,
        
        -- Ð¡Ð¾ÑÑ‚Ð¾ÑÐ½Ð¸Ðµ ÑÑ„Ñ„ÐµÐºÑ‚Ð¾Ð²
        Effects = {}
    }
    
    -- Ð¡Ð¾Ñ…Ñ€Ð°Ð½ÑÐµÐ¼ ÑÐ¾ÑÑ‚Ð¾ÑÐ½Ð¸Ðµ Ð²ÑÐµÑ… Ð²Ð¸Ð·ÑƒÐ°Ð»ÑŒÐ½Ñ‹Ñ… ÑÑ„Ñ„ÐµÐºÑ‚Ð¾Ð² (Ñ‚Ð¾Ð»ÑŒÐºÐ¾ Ñ‚Ðµ, Ñƒ ÐºÐ¾Ñ‚Ð¾Ñ€Ñ‹Ñ… ÐµÑÑ‚ÑŒ ÑÐ²Ð¾Ð¹ÑÑ‚Ð²Ð¾ Enabled)
    for _, effect in pairs(lighting:GetChildren()) do
        if effect:IsA("BlurEffect") or effect:IsA("SunRaysEffect") or 
           effect:IsA("ColorCorrectionEffect") or effect:IsA("BloomEffect") or
           effect:IsA("DepthOfFieldEffect") then
            originalSettings.Effects[effect] = {
                Enabled = effect.Enabled,
                Parent = effect.Parent
            }
        elseif effect:IsA("Atmosphere") then
            -- Ð”Ð»Ñ Atmosphere ÑÐ¾Ñ…Ñ€Ð°Ð½ÑÐµÐ¼ Ð´Ñ€ÑƒÐ³Ð¸Ðµ ÑÐ²Ð¾Ð¹ÑÑ‚Ð²Ð°, Ñ‚Ð°Ðº ÐºÐ°Ðº Ñƒ Ð½ÐµÐ³Ð¾ Ð½ÐµÑ‚ Enabled
            originalSettings.Effects[effect] = {
                Density = effect.Density,
                Offset = effect.Offset,
                Color = effect.Color,
                Decay = effect.Decay,
                Glare = effect.Glare,
                Haze = effect.Haze,
                Parent = effect.Parent
            }
        end
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð²Ð¾ÑÑÑ‚Ð°Ð½Ð¾Ð²Ð»ÐµÐ½Ð¸Ñ Ð²ÑÐµÑ… Ð½Ð°ÑÑ‚Ñ€Ð¾ÐµÐº
local function restoreOriginalSettings()
    local lighting = game:GetService("Lighting")
    local settings = settings()
    
    -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸ Ð¾ÑÐ²ÐµÑ‰ÐµÐ½Ð¸Ñ
    pcall(function()
        lighting.Ambient = originalSettings.Ambient or Color3.new(0.5, 0.5, 0.5)
        lighting.Brightness = originalSettings.Brightness or 1
        lighting.GlobalShadows = originalSettings.GlobalShadows or true
        lighting.OutdoorAmbient = originalSettings.OutdoorAmbient or Color3.new(0.5, 0.5, 0.5)
        lighting.ClockTime = originalSettings.ClockTime or 14
        lighting.FogStart = originalSettings.FogStart or 0
        lighting.FogEnd = originalSettings.FogEnd or 100000
        lighting.FogColor = originalSettings.FogColor or Color3.new(1, 1, 1)
        lighting.ExposureCompensation = originalSettings.ExposureCompensation or 0
    end)
    
    -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸ Ð³Ñ€Ð°Ñ„Ð¸ÐºÐ¸
    pcall(function()
        settings.Rendering.QualityLevel = originalSettings.QualityLevel or 10
        settings.Rendering.EagerBulkExecution = originalSettings.EagerBulkExecution or false
    end)
    
    -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ ÑÑ„Ñ„ÐµÐºÑ‚Ñ‹
    for effect, data in pairs(originalSettings.Effects or {}) do
        if effect and data.Parent then
            pcall(function()
                if effect:IsA("Atmosphere") then
                    -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ ÑÐ²Ð¾Ð¹ÑÑ‚Ð²Ð° Atmosphere
                    effect.Density = data.Density or 0
                    effect.Offset = data.Offset or 0
                    effect.Color = data.Color or Color3.new(1, 1, 1)
                    effect.Decay = data.Decay or Color3.new(1, 1, 1)
                    effect.Glare = data.Glare or 0
                    effect.Haze = data.Haze or 0
                else
                    -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð¾Ð±Ñ‹Ñ‡Ð½Ñ‹Ðµ ÑÑ„Ñ„ÐµÐºÑ‚Ñ‹
                    effect.Enabled = data.Enabled
                end
            end)
        end
    end
    
    originalSettings = {}
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¿Ñ€Ð¸Ð¼ÐµÐ½ÐµÐ½Ð¸Ñ Stop Rendering
local function applyStopRendering()
    if StopRenderingEnabled then
        -- Ð¡Ð¾Ñ…Ñ€Ð°Ð½ÑÐµÐ¼ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»ÑŒÐ½Ñ‹Ðµ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸
        saveOriginalSettings()
        
        local lighting = game:GetService("Lighting")
        
        -- ÐœÐ¸Ð½Ð¸Ð¼Ð¸Ð·Ð¸Ñ€ÑƒÐµÐ¼ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸ Ð³Ñ€Ð°Ñ„Ð¸ÐºÐ¸
        pcall(function()
            settings().Rendering.QualityLevel = 1
            lighting.GlobalShadows = false
            lighting.FogEnd = 9e9
            
            -- ÐžÑ‚ÐºÐ»ÑŽÑ‡Ð°ÐµÐ¼ Ñ€Ð°Ð·Ð»Ð¸Ñ‡Ð½Ñ‹Ðµ Ð²Ð¸Ð·ÑƒÐ°Ð»ÑŒÐ½Ñ‹Ðµ ÑÑ„Ñ„ÐµÐºÑ‚Ñ‹ (Ñ‚Ð¾Ð»ÑŒÐºÐ¾ Ñ‚Ðµ, Ñƒ ÐºÐ¾Ñ‚Ð¾Ñ€Ñ‹Ñ… ÐµÑÑ‚ÑŒ ÑÐ²Ð¾Ð¹ÑÑ‚Ð²Ð¾ Enabled)
            for _, effect in pairs(lighting:GetChildren()) do
                if effect:IsA("BlurEffect") or effect:IsA("SunRaysEffect") or 
                   effect:IsA("ColorCorrectionEffect") or effect:IsA("BloomEffect") or
                   effect:IsA("DepthOfFieldEffect") then
                    pcall(function()
                        effect.Enabled = false
                    end)
                elseif effect:IsA("Atmosphere") then
                    -- Ð”Ð»Ñ Atmosphere ÑƒÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð½ÑƒÐ»ÐµÐ²Ñ‹Ðµ Ð·Ð½Ð°Ñ‡ÐµÐ½Ð¸Ñ
                    pcall(function()
                        effect.Density = 0
                        effect.Offset = 0
                        effect.Glare = 0
                        effect.Haze = 0
                    end)
                end
            end
            
            -- Ð’ÐºÐ»ÑŽÑ‡Ð°ÐµÐ¼ Ð¾Ð¿Ñ‚Ð¸Ð¼Ð¸Ð·Ð°Ñ†Ð¸ÑŽ Ñ€ÐµÐ½Ð´ÐµÑ€Ð¸Ð½Ð³Ð°
            settings().Rendering.EagerBulkExecution = true
        end)
        
        -- Ð¡Ð¾Ð·Ð´Ð°ÐµÐ¼ Ñ‡ÐµÑ€Ð½Ñ‹Ð¹ ÑÐºÑ€Ð°Ð½
        blackScreen = Instance.new("ScreenGui")
        blackScreen.Name = "StopRenderingScreen"
        blackScreen.IgnoreGuiInset = true
        blackScreen.ResetOnSpawn = false
        
        local blackFrame = Instance.new("Frame")
        blackFrame.Size = UDim2.new(1, 0, 1, 0)
        blackFrame.BackgroundColor3 = Color3.new(0, 0, 0)
        blackFrame.BorderSizePixel = 0
        blackFrame.Parent = blackScreen
        
        blackScreen.Parent = game:GetService("CoreGui")
        
        Library:Notify({
            Title = "Stop Rendering Enabled",
            Description = "Graphics minimized for maximum FPS",
            Time = 3
        })
    else
        -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»ÑŒÐ½Ñ‹Ðµ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸
        restoreOriginalSettings()
        
        -- Ð£Ð´Ð°Ð»ÑÐµÐ¼ Ñ‡ÐµÑ€Ð½Ñ‹Ð¹ ÑÐºÑ€Ð°Ð½
        pcall(function()
            if blackScreen then
                blackScreen:Destroy()
                blackScreen = nil
            end
        end)
        
        Library:Notify({
            Title = "Stop Rendering Disabled",
            Description = "Graphics restored to original settings",
            Time = 3
        })
    end
end

-- Ð¢Ð¾Ð³Ð³Ð» Ð´Ð»Ñ Stop Rendering
local StopRenderingToggle = VisualTab:AddToggle("StopRendering", {
    Text = "ðŸ”º Stop Rendering",
    Default = false,
    Callback = function(Value)
        StopRenderingEnabled = Value
        print("Stop Rendering enabled:", Value)
        applyStopRendering()
    end
})

