-- ÐŸÐµÑ€ÐµÐ¼ÐµÐ½Ð½Ñ‹Ðµ Ð´Ð»Ñ ESP
local ESPEnabled = false
local ESPColor = Color3.fromRGB(255, 0, 0)
local ESPConnections = {}
local ESPObjects = {}

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ ÑÐ¾Ð·Ð´Ð°Ð½Ð¸Ñ ESP Ð´Ð»Ñ Ð¸Ð³Ñ€Ð¾ÐºÐ°
local function createESP(player)
    if ESPObjects[player] then return end
    
    local character = player.Character
    if not character then return end
    
    local highlight = Instance.new("Highlight")
    highlight.Name = "CubeESP"
    highlight.FillColor = ESPColor
    highlight.OutlineColor = ESPColor
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    highlight.Parent = character
    
    local billboard = Instance.new("BillboardGui")
    billboard.Name = "CubeESPInfo"
    billboard.AlwaysOnTop = true
    billboard.Size = UDim2.new(0, 200, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.Parent = character
    
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Text = player.Name
    nameLabel.TextColor3 = ESPColor
    nameLabel.TextSize = 14
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.BackgroundTransparency = 1
    nameLabel.Size = UDim2.new(1, 0, 0.5, 0)
    nameLabel.Position = UDim2.new(0, 0, 0, 0)
    nameLabel.Parent = billboard
    
    local distanceLabel = Instance.new("TextLabel")
    distanceLabel.Text = "Distance: 0"
    distanceLabel.TextColor3 = ESPColor
    distanceLabel.TextSize = 12
    distanceLabel.Font = Enum.Font.Gotham
    distanceLabel.BackgroundTransparency = 1
    distanceLabel.Size = UDim2.new(1, 0, 0.5, 0)
    distanceLabel.Position = UDim2.new(0, 0, 0.5, 0)
    distanceLabel.Parent = billboard
    
    ESPObjects[player] = {
        Highlight = highlight,
        Billboard = billboard,
        NameLabel = nameLabel,
        DistanceLabel = distanceLabel
    }
    
    -- ÐžÐ±Ñ€Ð°Ð±Ð¾Ñ‚Ñ‡Ð¸Ðº Ð¾Ð±Ð½Ð¾Ð²Ð»ÐµÐ½Ð¸Ñ Ñ€Ð°ÑÑÑ‚Ð¾ÑÐ½Ð¸Ñ
    local connection = RunService.Heartbeat:Connect(function()
        if not ESPEnabled or not player or not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
            return
        end
        
        local localPlayer = Players.LocalPlayer
        if not localPlayer or not localPlayer.Character or not localPlayer.Character:FindFirstChild("HumanoidRootPart") then
            return
        end
        
        local playerPos = player.Character.HumanoidRootPart.Position
        local localPos = localPlayer.Character.HumanoidRootPart.Position
        local distance = (playerPos - localPos).Magnitude
        
        if ESPObjects[player] and ESPObjects[player].DistanceLabel then
            ESPObjects[player].DistanceLabel.Text = "Distance: " .. math.floor(distance)
        end
    end)
    
    table.insert(ESPConnections, connection)
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ ÑƒÐ´Ð°Ð»ÐµÐ½Ð¸Ñ ESP Ð¸Ð³Ñ€Ð¾ÐºÐ°
local function removeESP(player)
    if ESPObjects[player] then
        if ESPObjects[player].Highlight then
            ESPObjects[player].Highlight:Destroy()
        end
        if ESPObjects[player].Billboard then
            ESPObjects[player].Billboard:Destroy()
        end
        ESPObjects[player] = nil
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¾Ð±Ð½Ð¾Ð²Ð»ÐµÐ½Ð¸Ñ Ñ†Ð²ÐµÑ‚Ð° ESP
local function updateESPColor()
    for player, espData in pairs(ESPObjects) do
        if espData.Highlight then
            espData.Highlight.FillColor = ESPColor
            espData.Highlight.OutlineColor = ESPColor
        end
        if espData.NameLabel then
            espData.NameLabel.TextColor3 = ESPColor
        end
        if espData.DistanceLabel then
            espData.DistanceLabel.TextColor3 = ESPColor
        end
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð²ÐºÐ»ÑŽÑ‡ÐµÐ½Ð¸Ñ/Ð²Ñ‹ÐºÐ»ÑŽÑ‡ÐµÐ½Ð¸Ñ ESP
local function updateESP()
    -- ÐžÑ‡Ð¸Ñ‰Ð°ÐµÐ¼ Ð¿Ñ€ÐµÐ´Ñ‹Ð´ÑƒÑ‰Ð¸Ðµ ÑÐ¾ÐµÐ´Ð¸Ð½ÐµÐ½Ð¸Ñ
    for _, connection in ipairs(ESPConnections) do
        connection:Disconnect()
    end
    ESPConnections = {}
    
    -- ÐžÑ‡Ð¸Ñ‰Ð°ÐµÐ¼ Ð¿Ñ€ÐµÐ´Ñ‹Ð´ÑƒÑ‰Ð¸Ðµ ESP Ð¾Ð±ÑŠÐµÐºÑ‚Ñ‹
    for player, espData in pairs(ESPObjects) do
        removeESP(player)
    end
    ESPObjects = {}
    
    if ESPEnabled then
        -- Ð¡Ð¾Ð·Ð´Ð°ÐµÐ¼ ESP Ð´Ð»Ñ Ð²ÑÐµÑ… ÑÑƒÑ‰ÐµÑÑ‚Ð²ÑƒÑŽÑ‰Ð¸Ñ… Ð¸Ð³Ñ€Ð¾ÐºÐ¾Ð²
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= Players.LocalPlayer then
                if player.Character then
                    createESP(player)
                end
                
                -- ÐžÐ±Ñ€Ð°Ð±Ð¾Ñ‚Ñ‡Ð¸Ðº Ð¿Ð¾ÑÐ²Ð»ÐµÐ½Ð¸Ñ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð°
                local connection = player.CharacterAdded:Connect(function(character)
                    wait(1) -- Ð–Ð´ÐµÐ¼ Ð¿Ð¾Ð»Ð½Ð¾Ð¹ Ð·Ð°Ð³Ñ€ÑƒÐ·ÐºÐ¸ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð°
                    if ESPEnabled then
                        createESP(player)
                    end
                end)
                table.insert(ESPConnections, connection)
            end
        end
        
        -- ÐžÐ±Ñ€Ð°Ð±Ð¾Ñ‚Ñ‡Ð¸Ðº Ð½Ð¾Ð²Ñ‹Ñ… Ð¸Ð³Ñ€Ð¾ÐºÐ¾Ð²
        local playerAddedConnection = Players.PlayerAdded:Connect(function(player)
            if ESPEnabled then
                local connection = player.CharacterAdded:Connect(function(character)
                    wait(1) -- Ð–Ð´ÐµÐ¼ Ð¿Ð¾Ð»Ð½Ð¾Ð¹ Ð·Ð°Ð³Ñ€ÑƒÐ·ÐºÐ¸ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð°
                    if ESPEnabled then
                        createESP(player)
                    end
                end)
                table.insert(ESPConnections, connection)
            end
        end)
        table.insert(ESPConnections, playerAddedConnection)
        
        -- ÐžÐ±Ñ€Ð°Ð±Ð¾Ñ‚Ñ‡Ð¸Ðº ÑƒÑˆÐµÐ´ÑˆÐ¸Ñ… Ð¸Ð³Ñ€Ð¾ÐºÐ¾Ð²
        local playerRemovingConnection = Players.PlayerRemoving:Connect(function(player)
            removeESP(player)
        end)
        table.insert(ESPConnections, playerRemovingConnection)
    end
end

-- Ð¢Ð¾Ð³Ð³Ð» Ð´Ð»Ñ ESP Ð² VisualTab
local ESPToggle = VisualTab:AddToggle("ESP", {
    Text = "ðŸŒ Player ESP",
    Default = false,
    Callback = function(Value)
        ESPEnabled = Value
        print("ESP enabled:", Value)
        updateESP()
    end
})

