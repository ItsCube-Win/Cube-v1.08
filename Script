-- ÐŸÐµÑ€ÐµÐ¼ÐµÐ½Ð½Ñ‹Ðµ Ð´Ð»Ñ God Mode
local GodModeEnabled = false
local godModeConnections = {}

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð²ÐºÐ»ÑŽÑ‡ÐµÐ½Ð¸Ñ God Mode
local function enableGodMode(character)
    if not character then return end
    
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if not humanoid then return end
    
    -- Ð¡Ð¾Ñ…Ñ€Ð°Ð½ÑÐµÐ¼ Ð¾Ñ€Ð¸Ð³Ð¸Ð½Ð°Ð»ÑŒÐ½Ð¾Ðµ Ð¼Ð°ÐºÑÐ¸Ð¼Ð°Ð»ÑŒÐ½Ð¾Ðµ Ð·Ð´Ð¾Ñ€Ð¾Ð²ÑŒÐµ
    local originalMaxHealth = humanoid.MaxHealth
    
    -- Ð£ÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð±ÐµÑÑÐ¼ÐµÑ€Ñ‚Ð¸Ðµ
    humanoid.MaxHealth = math.huge
    humanoid.Health = math.huge
    
    -- Ð—Ð°Ñ‰Ð¸Ñ‚Ð° Ð¾Ñ‚ Ð¿Ð¾Ð»ÑƒÑ‡ÐµÐ½Ð¸Ñ ÑƒÑ€Ð¾Ð½Ð°
    local healthConnection = humanoid.HealthChanged:Connect(function(newHealth)
        if GodModeEnabled and newHealth < math.huge then
            humanoid.Health = math.huge
        end
    end)
    
    -- Ð—Ð°Ñ‰Ð¸Ñ‚Ð° Ð¾Ñ‚ ÑÐ¼ÐµÑ€Ñ‚Ð¸
    local diedConnection = humanoid.Died:Connect(function()
        if GodModeEnabled then
            -- ÐÐµÐ¼ÐµÐ´Ð»ÐµÐ½Ð½Ð¾ Ð²Ð¾Ð·Ñ€Ð¾Ð¶Ð´Ð°ÐµÐ¼ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð°
            humanoid.Health = math.huge
        end
    end)
    
    -- Ð—Ð°Ñ‰Ð¸Ñ‚Ð° Ð¾Ñ‚ Ð¸Ð·Ð¼ÐµÐ½ÐµÐ½Ð¸Ñ Ð¼Ð°ÐºÑÐ¸Ð¼Ð°Ð»ÑŒÐ½Ð¾Ð³Ð¾ Ð·Ð´Ð¾Ñ€Ð¾Ð²ÑŒÑ
    local maxHealthConnection
    maxHealthConnection = humanoid:GetPropertyChangedSignal("MaxHealth"):Connect(function()
        if GodModeEnabled and humanoid.MaxHealth ~= math.huge then
            humanoid.MaxHealth = math.huge
        end
    end)
    
    -- ÐžÑ‚ÐºÐ»ÑŽÑ‡Ð°ÐµÐ¼ Ð²Ð¾Ð·Ð¼Ð¾Ð¶Ð½Ð¾ÑÑ‚ÑŒ ÑƒÐ¼Ð¸Ñ€Ð°Ñ‚ÑŒ Ð¾Ñ‚ Ð¿Ð°Ð´ÐµÐ½Ð¸Ñ
    if humanoid:FindFirstChild("FallDamage") then
        humanoid.FallDamage:Destroy()
    end
    
    table.insert(godModeConnections, healthConnection)
    table.insert(godModeConnections, diedConnection)
    table.insert(godModeConnections, maxHealthConnection)
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¾Ñ‚ÐºÐ»ÑŽÑ‡ÐµÐ½Ð¸Ñ God Mode
local function disableGodMode()
    -- ÐžÑ‚ÐºÐ»ÑŽÑ‡Ð°ÐµÐ¼ Ð²ÑÐµ ÑÐ¾ÐµÐ´Ð¸Ð½ÐµÐ½Ð¸Ñ
    for _, connection in ipairs(godModeConnections) do
        pcall(function()
            connection:Disconnect()
        end)
    end
    godModeConnections = {}
    
    -- Ð’Ð¾ÑÑÑ‚Ð°Ð½Ð°Ð²Ð»Ð¸Ð²Ð°ÐµÐ¼ Ð½Ð¾Ñ€Ð¼Ð°Ð»ÑŒÐ½Ð¾Ðµ Ð·Ð´Ð¾Ñ€Ð¾Ð²ÑŒÐµ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð°
    local character = player.Character
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            pcall(function()
                humanoid.MaxHealth = 100 -- Ð¡Ñ‚Ð°Ð½Ð´Ð°Ñ€Ñ‚Ð½Ð¾Ðµ Ð·Ð½Ð°Ñ‡ÐµÐ½Ð¸Ðµ
                humanoid.Health = math.min(humanoid.Health, humanoid.MaxHealth)
            end)
        end
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¾Ð±Ð½Ð¾Ð²Ð»ÐµÐ½Ð¸Ñ God Mode
local function updateGodMode()
    if GodModeEnabled then
        -- Ð’ÐºÐ»ÑŽÑ‡Ð°ÐµÐ¼ God Mode Ð´Ð»Ñ Ñ‚ÐµÐºÑƒÑ‰ÐµÐ³Ð¾ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð°
        if player.Character then
            enableGodMode(player.Character)
        end
        
        -- ÐžÐ±Ñ€Ð°Ð±Ð¾Ñ‚Ñ‡Ð¸Ðº Ð´Ð»Ñ Ð½Ð¾Ð²Ð¾Ð³Ð¾ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð°
        local characterConnection = player.CharacterAdded:Connect(function(character)
            if GodModeEnabled then
                wait(1) -- Ð–Ð´ÐµÐ¼ Ð¿Ð¾Ð»Ð½Ð¾Ð¹ Ð·Ð°Ð³Ñ€ÑƒÐ·ÐºÐ¸ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð°
                enableGodMode(character)
            end
        end)
        
        table.insert(godModeConnections, characterConnection)
        
        Library:Notify({
            Title = "God Mode Enabled",
            Description = "You are now invincible",
            Time = 3
        })
    else
        -- ÐžÑ‚ÐºÐ»ÑŽÑ‡Ð°ÐµÐ¼ God Mode
        disableGodMode()
        
        Library:Notify({
            Title = "God Mode Disabled",
            Description = "You can now take damage",
            Time = 3
        })
    end
end

-- Ð¢Ð¾Ð³Ð³Ð» Ð´Ð»Ñ God Mode
local GodModeToggle = GeneralTab:AddToggle("GodMode", {
    Text = "ðŸŒ  God Mode",
    Default = false,
    Callback = function(Value)
        GodModeEnabled = Value
        print("God Mode enabled:", Value)
        updateGodMode()
    end
})

-- ÐŸÐµÑ€ÐµÐ¼ÐµÐ½Ð½Ñ‹Ðµ Ð´Ð»Ñ Fly
local FlyEnabled = false
local FlyConnection = nil
local FlySpeed = 50
local BodyVelocity = nil
local BodyGyro = nil

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð°ÐºÑ‚Ð¸Ð²Ð°Ñ†Ð¸Ð¸/Ð´ÐµÐ°ÐºÑ‚Ð¸Ð²Ð°Ñ†Ð¸Ð¸ Ð¿Ð¾Ð»ÐµÑ‚Ð°
local function updateFly()
    if FlyConnection then
        FlyConnection:Disconnect()
        FlyConnection = nil
    end
    
    if BodyVelocity then
        BodyVelocity:Destroy()
        BodyVelocity = nil
    end
    
    if BodyGyro then
        BodyGyro:Destroy()
        BodyGyro = nil
    end
    
    if FlyEnabled then
        local character = player.Character
        if character then
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if humanoidRootPart then
                -- Ð¡Ð¾Ð·Ð´Ð°ÐµÐ¼ BodyGyro Ð´Ð»Ñ ÑÑ‚Ð°Ð±Ð¸Ð»Ð¸Ð·Ð°Ñ†Ð¸Ð¸
                BodyGyro = Instance.new("BodyGyro")
                BodyGyro.P = 10000
                BodyGyro.MaxTorque = Vector3.new(400000, 400000, 400000)
                BodyGyro.CFrame = humanoidRootPart.CFrame
                BodyGyro.Parent = humanoidRootPart
                
                -- Ð¡Ð¾Ð·Ð´Ð°ÐµÐ¼ BodyVelocity Ð´Ð»Ñ Ð´Ð²Ð¸Ð¶ÐµÐ½Ð¸Ñ
                BodyVelocity = Instance.new("BodyVelocity")
                BodyVelocity.Velocity = Vector3.new(0, 0, 0)
                BodyVelocity.MaxForce = Vector3.new(100000, 100000, 100000)
                BodyVelocity.Parent = humanoidRootPart
                
                -- Ð’ÐºÐ»ÑŽÑ‡Ð°ÐµÐ¼ PlatformStand Ð´Ð»Ñ Ð¾Ñ‚ÐºÐ»ÑŽÑ‡ÐµÐ½Ð¸Ñ Ñ„Ð¸Ð·Ð¸ÐºÐ¸
                local humanoid = character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid.PlatformStand = true
                end
                
                -- ÐžÐ±Ñ€Ð°Ð±Ð¾Ñ‚Ñ‡Ð¸Ðº Ð¿Ð¾Ð»ÐµÑ‚Ð°
                FlyConnection = RunService.Heartbeat:Connect(function()
                    if not FlyEnabled or not character or not humanoidRootPart then
                        return
                    end
                    
                    local camera = workspace.CurrentCamera
                    local direction = Vector3.new(0, 0, 0)
                    
                    -- ÐžÐ±Ñ€Ð°Ð±Ð¾Ñ‚ÐºÐ° ÐºÐ»Ð°Ð²Ð¸Ñˆ Ð´Ð²Ð¸Ð¶ÐµÐ½Ð¸Ñ
                    if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                        direction = direction + camera.CFrame.LookVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                        direction = direction - camera.CFrame.LookVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                        direction = direction - camera.CFrame.RightVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                        direction = direction + camera.CFrame.RightVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                        direction = direction + Vector3.new(0, 1, 0)
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                        direction = direction - Vector3.new(0, 1, 0)
                    end
                    
                    -- ÐŸÑ€Ð¸Ð¼ÐµÐ½ÑÐµÐ¼ ÑÐºÐ¾Ñ€Ð¾ÑÑ‚ÑŒ
                    if direction.Magnitude > 0 then
                        BodyVelocity.Velocity = direction.Unit * FlySpeed
                    else
                        BodyVelocity.Velocity = Vector3.new(0, 0, 0)
                    end
                    
                    -- ÐžÐ±Ð½Ð¾Ð²Ð»ÑÐµÐ¼ BodyGyro
                    BodyGyro.CFrame = camera.CFrame
                end)
            end
        end
    else
        -- Ð’Ñ‹ÐºÐ»ÑŽÑ‡Ð°ÐµÐ¼ PlatformStand Ð¿Ñ€Ð¸ Ð¾Ñ‚ÐºÐ»ÑŽÑ‡ÐµÐ½Ð¸Ð¸ Ð¿Ð¾Ð»ÐµÑ‚Ð°
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.PlatformStand = false
            end
        end
    end
end

-- ÐžÐ±Ñ€Ð°Ð±Ð¾Ñ‚Ñ‡Ð¸Ðº Ð¸Ð·Ð¼ÐµÐ½ÐµÐ½Ð¸Ñ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð° Ð´Ð»Ñ Ð¿Ð¾Ð»ÐµÑ‚Ð°
player.CharacterAdded:Connect(function(character)
    if FlyEnabled then
        wait(1) -- Ð–Ð´ÐµÐ¼ Ð¿Ð¾Ð»Ð½Ð¾Ð¹ Ð·Ð°Ð³Ñ€ÑƒÐ·ÐºÐ¸ Ð¿ÐµÑ€ÑÐ¾Ð½Ð°Ð¶Ð°
        updateFly()
    end
end)

-- Ð¢Ð¾Ð³Ð³Ð» Ð´Ð»Ñ Fly Ñ ÐºÐ»ÑŽÑ‡ÐµÐ²Ð¾Ð¹ Ð¿Ñ€Ð¸Ð²ÑÐ·ÐºÐ¾Ð¹
local FlyToggle = GeneralTab:AddToggle("Fly", {
    Text = "ðŸ”° Fly",
    Default = false,
    Callback = function(Value)
        FlyEnabled = Value
        print("Fly enabled:", Value)
        updateFly()
    end
})

-- Ð”Ð¾Ð±Ð°Ð²Ð»ÑÐµÐ¼ ÐºÐ»ÑŽÑ‡ÐµÐ²ÑƒÑŽ Ð¿Ñ€Ð¸Ð²ÑÐ·ÐºÑƒ Ð´Ð»Ñ Ð¿Ð¾Ð»ÐµÑ‚Ð°
local FlyKeybind = FlyToggle:AddKeyPicker("FlyKeybind", {
    Default = "F",
    Text = "Fly Keybind",
    Mode = "Toggle", -- Ð ÐµÐ¶Ð¸Ð¼: "Toggle", "Hold", "Always"
    
    -- Ð¡Ð¸Ð½Ñ…Ñ€Ð¾Ð½Ð¸Ð·Ð¸Ñ€ÑƒÐµÑ‚ ÑÐ¾ÑÑ‚Ð¾ÑÐ½Ð¸Ðµ Ñ‚Ð¾Ð³Ð³Ð»Ð° Ñ ÑÐ¾ÑÑ‚Ð¾ÑÐ½Ð¸ÐµÐ¼ ÐºÐ»ÑŽÑ‡ÐµÐ²Ð¾Ð¹ Ð¿Ñ€Ð¸Ð²ÑÐ·ÐºÐ¸
    SyncToggleState = true,
    
    Callback = function(Value)
        print("Fly keybind pressed, value:", Value)
        -- ÐÐµ Ð½ÑƒÐ¶Ð½Ð¾ Ð²Ñ‹Ð·Ñ‹Ð²Ð°Ñ‚ÑŒ updateFly() Ð·Ð´ÐµÑÑŒ, Ñ‚Ð°Ðº ÐºÐ°Ðº SyncToggleState = true
        -- Ð°Ð²Ñ‚Ð¾Ð¼Ð°Ñ‚Ð¸Ñ‡ÐµÑÐºÐ¸ Ð¸Ð·Ð¼ÐµÐ½Ð¸Ñ‚ ÑÐ¾ÑÑ‚Ð¾ÑÐ½Ð¸Ðµ Ñ‚Ð¾Ð³Ð³Ð»Ð°, ÐºÐ¾Ñ‚Ð¾Ñ€Ñ‹Ð¹ Ð²Ñ‹Ð·Ð¾Ð²ÐµÑ‚ updateFly()
    end
})

